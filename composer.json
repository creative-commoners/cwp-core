{
	"name": "cwp/cwp-installer",
	"description": "CWP Project Template",
	"require": {
		"cwp/cwp-recipe-basic": "dev-master",
		"cwp/cwp-recipe-blog": "dev-master",
		"cwp-themes/default": "dev-3.2-compat",
		"silverstripe/framework": "3.2.x-dev"
	},
	"require-dev": {
		"cwp/cwp-recipe-basic-dev": "dev-master"
	},
	"suggest": {
		"silverstripe/textextraction": "Required for document text searching within CWP"
	},
	"config": {
		"process-timeout": 900
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cwp.govt.nz/"
		},
		{
			"type": "vcs",
			"url": "https://gitlab.cwp.govt.nz/open-sausages/cwp-default-theme.git"
		}
	],
	"minimum-stability": "dev"
}

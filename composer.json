{
    "name": "cwp/cwp-core",
    "description": "CWP basic compatibility module. Use for migrations.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "require": {
        "silverstripe/framework": "^4.4@dev",
        "silverstripe/admin": "^1.4@dev",
        "silverstripe/hybridsessions": "^2",
        "silverstripe/environmentcheck": "^2",
        "silverstripe/auditor": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "CWP\\Core\\": "src/",
            "CWP\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "css",
            "javascript"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
